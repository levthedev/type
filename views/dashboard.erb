<div id='dashboard-wrapper'>
  <h1 id='header'> Lessons </h1>
  <div id='lessons'>
    <% lessons.map do |lesson| %>
      <a href='/lessons/<%=lesson.id%>'><div class='lesson'><%= lesson.text %></div></a>
    <% end %>
  </div>
</div>
<style>@import url('./stylesheets/dashboard.css');</style>
<script src="/js/dashboard.js"></script>
